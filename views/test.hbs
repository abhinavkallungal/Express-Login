   <section>
            <div class="container">
                <div class="row">
                    {{#each cards}}
                    <div class="col-lg-4 p-3">
                        <div class="card p-2">
                            <div class="img">
                                <img src="{{this.img}}" alt="dfd">
                            </div>
                            <div class="content">
                                <h4>{{this.name}}</h4>
                                <span>{{this.price}} </span>
                                <p>{{this.discription}} </p>
                                <a href="#" class="btn">button</a>
                            </div>
                        </div>
                    </div>
                   {{/each}}
                    
                </div>
            </div>
        </section>
        <section class="mt-5">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6">
                        <ul>
                            {{#each name}}
                            <li>{{this}}</li>
                            {{/each}}
                        </ul>
                    </div>
                    <div class="col-lg-6">
                        <ol>
                            {{#each name}}
                            <li>{{this}}</li>
                            {{/each}}
                        </ol>
                    </div>
                </div>
            </div>
        </section>
        <section>
            <div class="container">
                <div class="row">
                    <div class="col-lg-6">
                        <table class="table">
                            <thead class="thead-dark">
                              <tr>
                             
                                <th scope="col">Name</th>
                                <th scope="col">Age</th>
                                <th scope="col">Place</th>
                              </tr>
                            </thead>
                            <tbody>
                                {{#each details}}
                              <tr>
                               
                                <td>{{this.name}}</td>
                                <td>{{this.age}}</td>
                                <td>{{this.place}}</td>
                              </tr>
                              {{/each}}
                           
                            </tbody>
                          </table>
                          
                    </div>
                </div>
            </div>
        </section>